{
  "name": "qr-code-with-logo",
  "version": "1.0.18",
  "description": "qrCode with logo (image), 无jQuery依赖, 自由调整大小",
  "repository": {
    "type": "git",
    "url": "https://github.com/HerbLuo/qr-code-with-logo.git"
  },
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "unpkg": "lib/index.umd.js",
  "typings": "types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "scripts": {
    "dist:dev": "rollup --watch -c ./rollup.config.js",
    "dist": "rollup -c ./rollup.config.js",
    "test:dev": "webpack-dev-server --config test/module/webpack.dev.conf.js --mode development",
    "publish2npm": "npm run dist && npm publish"
  },
  "keywords": [
    "qr-code",
    "logo",
    "image",
    "qrCode",
    "size"
  ],
  "author": "herbluo",
  "license": "MIT",
  "dependencies": {
    "es6-promise": "^4.2.4",
    "qrcode": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-kotlish-also": "^1.0.5",
    "babel-preset-es2015-rollup": "^3.0.0",
    "p-single": "^1.0.1",
    "rollup": "^0.56.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-uglify": "^3.0.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "^3.0.0"
  }
}
